{
  "name": "eslint-plugin-chakra-ui",
  "version": "0.6.0",
  "description": "ESLint Plugin for Chakra UI",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "Chakra UI",
    "chakra-ui"
  ],
  "author": "Yuku Kotani (yukukotani@gmail.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Monchi/eslint-plugin-chakra-ui"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:tsc": "tsc --noEmit",
    "build": "rm -rf ./dist && swc ./src -d dist",
    "test": "uvu -r @swc-node/register src/__tests__",
    "test:watch": "watchlist src -- npm run test",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.10.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.127",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@typescript-eslint/types": "^5.10.0",
    "eslint": "^8.0.1",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "volta": {
    "node": "16.12.0",
    "npm": "8.3.0"
  }
}
